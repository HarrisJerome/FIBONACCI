package assignmentday2;

public class fibonacci {
	 static void printFibonacci(int n) {
	        int a = 0, b = 1;

	        for (int i = 0; i < n; i++) {
	            System.out.print(a + " ");

	            int nextTerm = a + b;
	            a = b;
	            b = nextTerm;
	        }
	    }

	    public static void main(String[] args) {
	        int n = 5; 

	        System.out.println("Fibonacci series up to " + n + " times:");
	        printFibonacci(n);
	    }
	}

